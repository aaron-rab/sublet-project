generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id          Int    @id @default(autoincrement())
  title       String
  description String
  rent        Int
  user_id     Int
  User        User   @relation(fields: [user_id], references: [id])
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  Post     Post[]
}
